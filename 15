public class CuboidVolumes {
  public static int findDifference(final int[] firstCuboid, final int[] secondCuboid) {
    int firstCuboidVol = firstCuboid[0]*firstCuboid[1]*firstCuboid[2];
    int secondCuboidVol = secondCuboid[0]*secondCuboid[1]*secondCuboid[2];
    return Math.abs(firstCuboidVol - secondCuboidVol);
  }
}
